<body class="text-center">
    <form class="form-signin" action="/login" method="POST">
        <img class="mb-4" src="/images/logo1.png" alt="" width="120" height="120">
        <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
        <label for="inputEmail" class="sr-only">Email address</label>
        <input type="text" id="inputEmail" name="username" class="form-control" placeholder="Email address" autofocus required>
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" name="password" class="form-control" placeholder="Password" required>
        <div class="checkbox mb-3">
        </div>
        <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
        <div>
            <br>
            <a href="/register"> Sign up!</a>
        </div>

        <p class="mt-5 mb-3 text-muted">&copy; Güid 2018-2019</p>
    </form>
